/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    // Put your code here:

    Not(in=sel, out=x);
    And(a=sel, b=b, out=y);
    And(a=x, b=a, out=z);
    Or(a=y, b=z, out=out);
}